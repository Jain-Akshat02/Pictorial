{   
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/server.js"
        },
        {
            "src": "/auth(.*)",
            "dest": "/server/server.js"
        },
        {
            "src": "/photos(.*)",
            "dest": "/server/server.js"
        },
        {
            "src": "/images(.*)",
            "dest": "/server/server.js"
        }
    ]
}